{
    "extends": "@vue/tsconfig/tsconfig.dom.json",
    "compilerOptions": {
        "composite": true,
        "strict": false,
        "noEmitOnError": false,
        "skipLibCheck": true,
        "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.legacy.tsbuildinfo",
        "paths": {
            "@/legacy/*": [
                "./src/legacy/*"
            ],
            "@/composables/*": [
                "./src/composables/*"
            ],
            "@/components/*": [
                "./src/components/*"
            ],
            "@/views/*": [
                "./src/views/*"
            ],
            "@/types/*": [
                "./src/types/*"
            ],
            "@/utils/*": [
                "./src/utils/*"
            ],
            "@/*": [
                "./src/*"
            ]
        }
    },
    "include": [
        "env.d.ts",
        "src/**/*",
        "src/**/*.vue"
    ],
    "exclude": [
        "src/features/**/*",
        "src/**/__tests__/*"
    ]
}